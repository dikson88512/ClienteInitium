@{
    ViewData["Title"] = "Asignar Cliente a Cola";
}

<div class="text-center">
    <h1 class="display-4">Registro de Clientes</h1>
   
</div>

<form>
    <div class="form-group">
        <div class="col-4">
            <label for="txtIdCliente">Id</label>
            <input type="text" class="form-control" id="txtIdCliente" aria-describedby="emailHelp" placeholder="Id Cliente">
            
        </div>
    </div>
    <div class="form-group">
        <div class="col-4">
            <label for="txtNombreCliente">Nombre Cliente</label>
            <input type="text" class="form-control" id="txtNombreCliente" placeholder="Nombre Completo">
        </div>
    </div>
    
    <button type="button" class="btn btn-primary" id="txtGuardar">Guardar</button>
</form>
<script src="https://code.jquery.com/jquery-3.5.1.js"
        integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc="
        crossorigin="anonymous">

</script>

<script src="~/js/Cliente.js" type="text/javascript"></script>
